{
    "name": "@kolben/ra-language-swedish",
    "version": "4.0.0",
    "description": "Swedish messages for react-admin, the frontend framework for building admin applications on top of REST/GraphQL services",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kolben/ra-language-swedish.git"
    },
    "files": [
        "*.md",
        "lib",
        "esm",
        "src",
        "tsconfig.json",
        "tsconfig-ra.json"
    ],
    "main": "lib/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "scripts": {
        "build": "npm run build-cjs && npm run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --outDir esm --module es2015 --watch",
        "prepack": "npm run build"
    },
    "dependencies": {
        "ra-core": "^4.0.0"
    },
    "keywords": [
        "react",
        "react-admin",
        "admin-on-rest",
        "translation"
    ],
    "author": "Sigrid Stj√§rnholm",
    "contibutors": [
        "Stefan Wallin",
        "Jonas Kolben"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kolben/ra-language-swedish/issues"
    },
    "homepage": "https://github.com/kolben/ra-language-swedish#readme",
    "devDependencies": {
        "rimraf": "^3.0.2",
        "typescript": "^4.6.3"
    },
    "publishConfig": {
        "access": "public"
    }
}
